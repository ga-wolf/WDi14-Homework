<%= form_for @project do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title, :required => true %>
  </p>
  <p>
    <%= f.label :description %> <br>
    <%= f.text_area :description, size: "24x6", :required => true %>
  </p>
  <p>Select project members</p>
  <p>
    <% user_arr = [] %>
    <% @users.each do |u| %>
      <% x = [u.username, u.id] %>
      <% user_arr.push x %>
    <% end %>
    <%= f.select( :user_ids, user_arr, {}, {:multiple => true}) %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
